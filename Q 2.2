ORG 0000H
MOV 50H, #04H
MOV 51H, #04H
COMPARE:
    MOV A, 50H
    JZ A_IS_ZERO
    MOV A, 51H
    JZ B_IS_ZERO
    DEC 50H
    DEC 51H
    SJMP COMPARE
A_IS_ZERO:
    MOV A, 51H
    JZ BOTH_EQUAL
    MOV R0, #0FFH
    SJMP DONE
B_IS_ZERO:
    MOV R0, #01H
    SJMP DONE
BOTH_EQUAL:
    MOV R0, #00H
DONE:
    HERE: SJMP HERE
END
